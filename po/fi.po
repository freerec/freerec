#: ../freerec.desktop.in.h:1
msgid "Audio recorder and song player"
msgstr "Äänitysohjelma ja laulusoitin"

#: ../freerec.desktop.in.h:2 ../ui/freerec.ui.in.h:3
msgid "FreeRec"
msgstr "FreeRec"

#: ../ui/freerec.ui.in.h:1
msgid "<b>Recording</b>"
msgstr "<b>Äänitys</b>"

#: ../ui/freerec.ui.in.h:2
msgid "<b>Songs</b>"
msgstr "<b>Laulut</b>"

#: ../ui/freerec.ui.in.h:4
msgid "Open Directory"
msgstr "Avaa hakemisto"

#: ../ui/freerec.ui.in.h:5
msgid "Open the directory containing the recorded files"
msgstr "Avaa hakemisto, johon äänitteet on tallennettu"

#: ../ui/freerec.ui.in.h:6
msgid "Pause"
msgstr "Tauko"

#: ../ui/freerec.ui.in.h:7
msgid "Pause recording"
msgstr "Pidä tauko äänityksessä"

#: ../ui/freerec.ui.in.h:8
msgid "Pause the song"
msgstr "Pidä tauko laulun toistossa"

#: ../ui/freerec.ui.in.h:9
msgid "Play"
msgstr "Toista"

#: ../ui/freerec.ui.in.h:10
msgid "Record"
msgstr "Äänitä"

#: ../ui/freerec.ui.in.h:11
msgid "Start or continue playing the song"
msgstr "Aloita laulun toisto tai jatka toistoa"

#: ../ui/freerec.ui.in.h:12
msgid "Start or continue recording"
msgstr "Aloita äänitys tai jatka äänitystä"

#: ../ui/freerec.ui.in.h:13
msgid "Stop"
msgstr "Pysäytä"

#: ../ui/freerec.ui.in.h:14
msgid "Stop recording"
msgstr "Lopeta äänitys"

#: ../ui/freerec.ui.in.h:15
msgid "Stop the song"
msgstr "Lopeta laulun toisto"

#: freerec:95
msgid "Well, this is embarrassing. FreeRec crashed."
msgstr "Tämäpä noloa. FreeRec kaatui."

#: freerec:97
msgid ""
"Please convey the following error message to the developers:\n"
"\n"
"%s"
msgstr ""
"Ole hyvä ja välitä seuraava virheilmoitus kehittäjille:\n"
"\n"
"%s"
